- const navItems = {"#features": "Преимущества", "#specs": "Характеристики", "#for-whom": "Для кого"}


header.page-header

  nav.main-nav.container

    a(href="#intro").logo._logo= "Логотип " + companyName + ", наверх"
      svg( role="img", focusable="false", width="156", height="40")._img
        use(xlink:href="#logo__gear", width="40", height="40", x="0")._gear
        use(xlink:href="#logo__text", width="114", height="29", x="44", y="5")._text

    button._toggle
      svg(viewBox="0 0 100 100", width="20", height="20").hamburger-menu
        rect(x="0", y="0", width="100%", height="20%", rx="5%")._top
        rect(x="0", y="40%", width="100%", height="20%", rx="5%")._middle
        rect(x="0", y="80%", width="100%", height="20%", rx="5%")._bottom

    ul._list
      each val, key in navItems
        li._item
          a(href=key)._link= val
